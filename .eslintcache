string().min(2).max(1024).required(),\n    ingredients: Yup.array().of(\n      Yup.object().shape({\n        name: Yup.string().min(2).max(255).required(),\n        amount: Yup.number().min(2).max(50).required(),\n      })\n    ),\n    prepTime: Yup.string().min(2).max(10).required(),\n    difficulty: Yup.number().min(1).max(5).required(),\n    image: Yup.string().min(11).max(1024).url(),\n  });\n\n  const MyTextField = ({ label, type, ...props }) => {\n    const [field, meta] = useField(props);\n    const error = meta.error && meta.touched ? meta.error : \"\";\n\n    return <Input {...field} label={label} type={type} error={error} />;\n  };\n\n  const MyTextArea = ({ label, type, ...props }) => {\n    const [field, meta] = useField(props);\n    const error = meta.error && meta.touched ? meta.error : \"\";\n\n    return <TextArea {...field} label={label} type={type} error={error} />;\n  };\n  const MyArrInput = ({ field, type, label }) => {\n    return (\n      <>\n        <Input {...field} type={type} label={label} />\n      </>\n    );\n  };\n\n  return (\n    <div className=\"container createRecipe-container\">\n      <div className=\"row content-box my-5\">\n        <div className=\"col-lg-5 mt-5\">\n          <h1 className=\"fPrimary title-fSize txt-red\">New recipe</h1>\n          <p className=\"fSecondary-roughThin subTitle-fSize\">\n            Share your <br />\n            <span className=\"fSecondary-rough  txt-green\">best </span>\n            recipes\n          </p>\n          <img\n            className=\"order-img\"\n            src=\"/images/Professional-chef.png\"\n            alt=\"\"\n          />\n        </div>\n        <div className=\"col-lg-7\">\n          <div className=\"create-recipe-box \">\n            <h1 className=\"fPrimary mb-5 display-4\">Join in</h1>\n            <Formik\n              initialValues={formValues || initialValues}\n              validationSchema={validationSchema}\n              enableReinitialize\n              onSubmit={async (\n                values,\n                { setSubmitting, resetForm, setErrors }\n              ) => {\n                const data = { ...values };\n                console.log(\"submitting\", data);\n                setSubmitting(true);\n                try {\n                  if (!data.image) delete data.image;\n                  await recipeService.editRecipe(data);\n                  toast(\"Thank you for sharing\");\n                  history.replace(\"/my-recipes\");\n                } catch (ex) {\n                  if (ex.response && ex.response.status === 400) {\n                    setErrors({ email: ex.response.data });\n                  }\n                }\n              }}\n            >\n              {({ values, error, isSubmitting, handleSubmit }) => (\n                <Form onSubmit={handleSubmit}>\n                  <div className=\"form-group\">\n                    {<MyTextField name=\"title\" label=\"Title\" error={error} />}\n                  </div>\n                  <div className=\"form-group\">\n                    <MyTextArea\n                      name=\"instructions\"\n                      label=\"Instructions\"\n                      error={error}\n                    />\n                  </div>\n                  <div className=\"form-group\">\n                    <FieldArray name=\"ingredients\">\n                      {({ push, remove }) => (\n                        <div>\n                          {values.ingredients &&\n                            values.ingredients.length > 0 &&\n                            values.ingredients.map((ingredient, index) => {\n                              return (\n                                <div key={index}>\n                                  <div className=\"row\">\n                                    <div className=\"col-5\">\n                                      <Field\n                                        name={`{ingredients[${index}].name}`}\n                                        label=\"Ingredient\"\n                                        component={MyArrInput}\n                                        value={ingredient.name}\n                                      />\n                                    </div>\n                                    <div className=\"col-2\">\n                                      <Field\n                                        name={`ingredients[${index}].amount`}\n                                        label=\"Amount\"\n                                        type=\"number\"\n                                        component={MyArrInput}\n                                        value={ingredient.amount}\n                                      />\n                                    </div>\n                                    <div className=\"col-5 my-auto\">\n                                      <button\n                                        className=\"btn btn-secondary fSecondary-regular mt-3\"\n                                        type=\"button\"\n                                        onClick={() => remove(index)}\n                                      >\n                                        remove\n                                      </button>\n                                    </div>\n                                  </div>\n                                </div>\n                              );\n                            })}\n                          <div className=\"col-4 my-auto\">\n                            <button\n                              className=\"btn btn-primary fSecondary-regular\"\n                              type=\"button\"\n                              onClick={() => push({ name: \"\", amount: \"\" })}\n                            >\n                              Add\n                            </button>\n                          </div>\n                        </div>\n                      )}\n                    </FieldArray>\n                  </div>\n\n                  <div className=\"form-group\">\n                    <MyTextField\n                      name=\"prepTime\"\n                      label=\"Prepare time\"\n                      error={error}\n                    />\n                  </div>\n                  <div className=\"form-group\">\n                    <MyTextField\n                      type=\"number\"\n                      name=\"difficulty\"\n                      label=\"Difficulty\"\n                      error={error}\n                    />\n                  </div>\n                  <div className=\"form-group\">\n                    <MyTextField name=\"image\" label=\"Image\" error={error} />\n                  </div>\n                  <button\n                    className=\"btn btn-primary fSecondary-regular\"\n                    disabled={isSubmitting}\n                    type=\"submit\"\n                  >\n                    Create\n                  </button>\n                  <Link\n                    className=\"btn btn-secondary ml-2 fSecondary-thin\"\n                    to=\"/my-recipes\"\n                  >\n                    Cancel\n                  </Link>\n                </Form>\n              )}\n            </Formik>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default RecipeEdit;\n","/Users/moriel/Desktop/HackerU/Projects/final project/boocooking-app/src/Viewport/view-port.jsx",[],{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":40,"column":6,"nodeType":"159","endLine":40,"endColumn":8,"suggestions":"160"},{"ruleId":"153","replacedBy":"161"},{"ruleId":"155","replacedBy":"162"},{"ruleId":"153","replacedBy":"163"},{"ruleId":"155","replacedBy":"164"},{"ruleId":"153","replacedBy":"165"},{"ruleId":"155","replacedBy":"166"},{"ruleId":"167","severity":1,"message":"168","line":16,"column":20,"nodeType":"169","messageId":"170","endLine":